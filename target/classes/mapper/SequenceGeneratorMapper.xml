<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mmall.dao.SequenceGeneratorMapper" >
  <select id="nextLongValue" parameterType="java.lang.String" resultType="long">
    SELECT ${sequenceName}.nextval from dual
  </select>
  <select id="nextIntValue" parameterType="java.lang.String" resultType="int">
    SELECT ${sequenceName}.nextval from dual
  </select>
  <select id="nextStringValue" parameterType="java.lang.String" resultType="string">
    SELECT ${sequenceName}.nextval from dual
  </select>

</mapper>